layer {
        name: "data"
        type: "Python"
        top: "qa"
        top: "qb"
        top: "im"
        top: "posdata"
        top: "labels"
        include {
                phase: TRAIN     
        }         
        python_param {                 
                module: 'rel_data_layer.layer_pairwise'              
                layer: 'RelDataLayer'                 
                param_str: '{"dataset": "reltrain.json", "batch_size": 25, "nclass": 100}'   
        }         
}                    
layer {           
        name: "data"                   
        type: "Python"                 
        top: "qa" 
        top: "qb" 
        top: "im" 
        top: "posdata"                 
        top: "labels"                  
        include { 
                phase: TEST            
        }         
        python_param {                 
                module: 'rel_data_layer.layer_pairwise'              
                layer: 'RelDataLayer'
                param_str: '{"dataset": "reltest.json", "batch_size": 25, "nclass": 100}'
        }         
}
# Appearance Subnet
layer {
	name: "conv1_1"
	type: "Convolution"
	bottom: "im"
	top: "conv1_1"
