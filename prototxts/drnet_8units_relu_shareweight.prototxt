# Spatial Cfg Subnet
layer {
	name: "conv1_p"
	type: "Convolution"
	bottom: "posdata"
	top: "conv1_p"
	param {
		lr_mult: 1
		decay_mult: 0
	}
	param {
		lr_mult: 1
		decay_mult: 0
	}
	convolution_param {
		num_output: 96
		kernel_size: 5
		pad: 2
		stride: 2
	}
}
layer {
	name: "relu1_p"
	type: "ReLU"
	bottom: "conv1_p"
	top: "conv1_p"
}
layer {
	name: "conv2_p"
	type: "Convolution"
	bottom: "conv1_p"
	top: "conv2_p"
	param {
		lr_mult: 1
		decay_mult: 0
	}
	param {
		lr_mult: 1
		decay_mult: 0
	}
	convolution_param {
		num_output: 128
		kernel_size: 5
		pad: 2
		stride: 2
	}
}
layer {
	name: "conv3_p"
	type: "Convolution"
	bottom: "conv2_p"
	top: "conv3_p"
	param {
		lr_mult: 1
		decay_mult: 0
	}
	param {
		lr_mult: 1
		decay_mult: 0
	}
	convolution_param {
		num_output: 256
		kernel_size: 5
	}
}
layer {
	name: "relu3_p"
	type: "ReLU"
	bottom: "conv3_p"
	top: "conv3_p"
}

# Combine features from subnets
layer {
	name: "concat1_c"
	type: "Python"
	bottom: "fc6_n"
	bottom: "conv3_p"
	top: "concat1_c"
	python_param {
		module: "customize_layers.concat_layer"
		layer: "Layer"
	}
}

layer {
	name: "fc7_c"
	type: "InnerProduct"
	bottom: "concat1_c"
	top: "fc7_c"
	param {
		lr_mult: 1
		decay_mult: 0
	}
	param {
		lr_mult: 2
		decay_mult: 0
	}
	inner_product_param {
		num_output: 6200
	}
}

layer {
	name: "relu2_c"
	type: "ReLU"
	bottom: "fc7_c"
	top: "fc7_c"
}
layer {
	name: "PhiR_0"
	type: "InnerProduct"
	bottom: "fc7_c"
	top: "q0r"
	param {
		lr_mult: 1
		decay_mult: 0
	}
	param {
		lr_mult: 2
		decay_mult: 0
	}
	inner_product_param {
		num_output: 4900
		weight_filler {
			type: "xavier"
		}
		bias_filler {
			type: "constant"
			value: 0
		}
	}
}

layer {
	name: "relu_0"
	type: "ReLU"
	bottom: "q0r"
	top: "qr0"
}

layer {
	name: "loss3"
	type: "SoftmaxWithLoss"
	top: "loss3"
	bottom: "qr0"
	bottom: "labels"
	include {
		phase: TRAIN
	}
	loss_weight: 1
}
layer {
	name: "softmax"
	type: "Softmax"
	bottom: "qr0"
	top: "pred"
	include {
		phase: TEST
	}
}
layer {
	name: "accuracy"
	type: "Accuracy"
	bottom: "pred"
	bottom: "labels"
	top: "top-1"
	include {
		phase: TEST
	}
	accuracy_param {
		top_k: 1
	}
}
layer {
	name: "accuracy_top5"
	type: "Accuracy"
	bottom: "pred"
	bottom: "labels"
	top: "top-5"
	include {
		phase: TEST
	}
	accuracy_param {
		top_k: 5
	}
}
